fn max(num1: 32, num2: i32, num3: i32) -> i32 {
    var res: i32;

    if (num1 > num2) {
        if (num1 > num3) {
            res = num1;
        } else {
            res = num3;
        }
    } else {
        if (num2 > num3) {
            res = num2;
        } else {
            res = num3;
        }
    }

    return res;
}

fn main() -> i32 {
    max(1, 2, 3);
    return 1;
}